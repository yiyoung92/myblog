#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/git/myblog/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_3e082ee02f2eea686a135d460756ea06/node_modules/vitepress/bin/node_modules:/mnt/d/git/myblog/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_3e082ee02f2eea686a135d460756ea06/node_modules/vitepress/node_modules:/mnt/d/git/myblog/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_3e082ee02f2eea686a135d460756ea06/node_modules:/mnt/d/git/myblog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/git/myblog/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_3e082ee02f2eea686a135d460756ea06/node_modules/vitepress/bin/node_modules:/mnt/d/git/myblog/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_3e082ee02f2eea686a135d460756ea06/node_modules/vitepress/node_modules:/mnt/d/git/myblog/node_modules/.pnpm/vitepress@1.6.3_@algolia+cl_3e082ee02f2eea686a135d460756ea06/node_modules:/mnt/d/git/myblog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vitepress@1.6.3_@algolia+cl_3e082ee02f2eea686a135d460756ea06/node_modules/vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../../../../../vitepress@1.6.3_@algolia+cl_3e082ee02f2eea686a135d460756ea06/node_modules/vitepress/bin/vitepress.js" "$@"
fi
